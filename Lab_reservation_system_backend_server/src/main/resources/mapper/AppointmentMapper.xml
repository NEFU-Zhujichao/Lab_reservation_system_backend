<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.lab_reservation_system_backend_server.mapper.AppointmentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.lab_reservation_system_backend_server.pojo.Appointment">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="uname" property="uname" />
        <result column="cid" property="cid" />
        <result column="cname" property="cname" />
        <result column="lab_name" property="labName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, uname, cid, cname, lab_name
    </sql>

</mapper>
